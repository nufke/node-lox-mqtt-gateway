{
    "winston": [{
        "Console": {
            "level": "debug",
            "colorize": true,
            "timestamp": true
        }
    }],
    "mqtt": {
        "host": "mqtt://<yourbrokerIP>:1883",
        "options": {
            "username": "<yourbrokerusername>",
            "password": "<yourbrokerpassword>",
