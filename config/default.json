{
    "winston": [{
        "Console": {
            "level": "debug",
            "colorize": true,
            "timestamp": true
        }
    }],
    "mqtt": {
        "host": "mqtt://<yourbrokerIP>:1883",
        "options": {
            "rejectUnauthorized": false,
            "username": "<yourbrokerusername>",
            "password": "<yourbrokerpassword>",
            "clientId": "lox_to_mqtt_gateway"
        }
    },
    "miniserver": {
        "host": "<yourminiserver>:80",
        "username": "<yourminiserverusername>",
        "password": "<yourminiserverpassword>",
        "readonly": false,
        "mqtt_prefix": "lox"
    }
}